func (t *{{ .Name }}) Unmarshal(decoder *protocol.Decoder, version int) error {
  var err error
{{- range $i, $f := .Fields | forVersion .Versions }}

{{- if .Type | isPrimitiveArray }}
  t.{{ $f.Name }}, err = decoder.{{ $f.Type | baseType | capitalize }}Array()
  if err != nil {
  	return err
  }
{{- end }}
{{- if .Type | isStructArray }}
// {{ $f.Name }}
if n, err := decoder.ArrayLength(); err != nil {
    return err
  } else if n >= 0 {
    t.{{ $f.Name }} = make({{ $f.Type }}{{ $.ApiKey}}, n)
    for i := 0; i < n; i++ {
      var item {{ $f.Type | baseType }}{{ $.ApiKey}}
      if err := (&item).Unmarshal(decoder, version); err != nil {
        return err
      }
      t.{{ $f.Name }}[i] = item
    }
  }
{{- end }}
{{- if $f.Type | isArray | not }}
  t.{{ $f.Name }}, err = decoder.{{ $f.Type | capitalize }}()
  if err != nil {
    return err
  }
{{- end }}
{{- end }}
  return err
}