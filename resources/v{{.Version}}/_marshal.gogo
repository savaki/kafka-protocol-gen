func (t {{ .Name }}) Marshal(encoder *protocol.Encoder) {
{{- range $i, $f := .Fields | forVersion .Version }}
{{- if .Type | isArray }}
  encoder.PutArray(len(t.{{ $f.Name }}), func(i int) { t.{{ $f.Name }}[i].Marshal(encoder) })
{{- end }}
{{- if .Type | isArray | not }}
  encoder.Put{{ .Type | capitalize }}(t.{{ $f.Name}})
{{- end }}
{{- end }}
}
